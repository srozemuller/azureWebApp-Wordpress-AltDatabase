{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "databaseName": {
            "value": "[mysqldatabase]"
        },
        "administratorLogin": {
            "value": "[mysqldbuser]"
        },
        "administratorLoginPassword": {
            "value": null
        },
        "mysqLocation": {
            "value": "West Europe"
        },
        "serverName": {
            "value": "[mysqldbserver]"
        },
        "skuCapacity": {
            "value": 2
        },
        "skuFamily": {
            "value": "Gen5"
        },
        "skuName": {
            "value": "B_Gen5_2"
        },
        "skuSizeMB": {
            "value": 102400
        },
        "skuTier": {
            "value": "Basic"
        },
        "storageMB": {
            "value": 102400
        },
        "backupRetentionDays": {
            "value": 7
        },
        "geoRedundantBackup": {
            "value": "Disabled"
        },
        "version": {
            "value": "8.0"
        },
        "collation": {
            "value": "utf8_general_ci"
        },
        "charset": {
            "value": "utf8"
        },
        "name": {
            "value": "[YOURWEBSITENAME]"
        },
        "hostingPlanName": {
            "value": "[APPSERVICEPLAN]"
        },
        "hostingEnvironment": {
            "value": ""
        },
        "location": {
            "value": "West Europe"
        },
        "serverFarmResourceGroup": {
            "value": "[YOURRESOURCEGROUP]"
        },
        "subscriptionId": {
            "value": "[SUBSCRIPTIONID]"
        }
    }
}
